	<config name="sounds">
		<append xpath="/Sounds">

			<SoundDataNode name="DroneRandom">
				<AudioSource name="Sounds/AudioSource_VO" />
				<Channel name="Mouth"/>
				<Priority name="3"/>
				<Noise ID="3" range="50" volume="10" time="10" muffled_when_crouched="0.65" heat_map_strength="0.05" heat_map_time="60" />
				<AudioClip ClipName="#DroneSounds?Idle1" />
				<AudioClip ClipName="#DroneSounds?Idle2" />
				<AudioClip ClipName="#DroneSounds?Idle3" />
				<AudioClip ClipName="#DroneSounds?Idle4" />
				<AudioClip ClipName="#DroneSounds?Idle5" />
				<LocalCrouchVolumeScale name="1.0" />
				<CrouchNoiseScale name="0.5" />
				<NoiseScale name="1" />
				<MaxVoices name="10" />
				<MaxRepeatRate name="3.111" />
			</SoundDataNode>
			<SoundDataNode name="DroneDie">
				<Channel name="Mouth"/>
				<Priority name="2"/>
				<AudioSource name="Sounds/AudioSource_VO" />
				<Noise ID="3" range="50" volume="10" time="10" muffled_when_crouched="0.65" heat_map_strength="0.05" heat_map_time="60" />
				<AudioClip ClipName="#DroneSounds?Die" />
				<LocalCrouchVolumeScale name="1.0" />
				<CrouchNoiseScale name="0.5" />
				<NoiseScale name="1" />
				<MaxVoices name="10" />
				<MaxRepeatRate name="1.111" />
			</SoundDataNode>

		</append>
	</config>
	<config name="items">
		<append xpath="/items">

			<item id="" name="handSphereII">
				<property name="Extends" value="handZombie"/>
				<property name="CreativeMode" value="None"/>
				<property class="Action0">
					<property name="DamageEntity" value="75"/>
					<property name="DamageBlock" value="40"/>
					<property name="Range" value="2.5"/>
					<property name="Sphere" value=".5"/>
					<property name="Sound_start" value="swoosh" />
					<property name="Buff" value="criticalBlunt,stunned,sprainedLeg" />
					<property name="Buff_chance" value="0.5,0.3,0.3" />
					<property name="DamageBonus.head" value="5" />
				</property>
			</item>
